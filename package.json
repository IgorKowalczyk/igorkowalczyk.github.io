    "clsx": "2.1.1",
    "contentlayer2": "0.5.3",
    "dotenv": "16.4.7",
    "geist": "1.3.1",
    "github-slugger": "2.0.0",
    "lucide-react": "0.468.0",
    "mdx-bundler": "10.0.3",
    "next": "15.1.1",
    "next-contentlayer2": "0.5.3",
    "next-nprogress-bar": "2.4.2",
    "next-themes": "0.4.4",
